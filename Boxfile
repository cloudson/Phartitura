web1: 
   name: phartitura
   document_root: public
   php_version: 5.4.14
   php_extensions: 
       - curl 
       - json
       - zip  
       - session 
   after_build:
    - "ls"
    - "cp config/parameters.ini.sample config/parameters.ini"
    - "if [ ! -f composer.phar ]; then curl -s http://getcomposer.org/installer | php; fi; php composer.phar install"



